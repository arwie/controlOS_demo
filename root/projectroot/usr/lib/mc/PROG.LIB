<?com_websocket(55003)?>



<?lib_prgStart()?>
	attach
	tool_select(tool_typeLaser)
	tool_grip(true)
end sub


<?com_prgReceive('this_websocket', 200)?>
	select case receive_
	case com_receiveMessage
		movePath()
	case com_receiveTimeout
		if not delta1_disabled then
			delta1_disable()
		end if
	end select
	
	com_clear()
	com_putLocationXyzr("/here",		here)
	com_send(this_websocket)
end sub


<?lib_prgStop()?>
	tool_select(tool_typeNone)
	delta1_disable()
end sub


<?lib_prgError()?>
	stop stoptype=ABORT
	if not prgRunning then
		prog_prgStop()
		prog_stopTrigger()
	end if
end sub


sub movePath
	dim i as long
	
	try
		delta1_enable()
		for i = 0 to com_size("/points")-1
			com_with("/points/"+str$(i))
			moves com_getLocationXyzr("path")
		next
		waitForMotion
	<?catchLog()?>
end sub
